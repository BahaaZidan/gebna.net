{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "gebna-backend",
	"compatibility_date": "2025-08-03",
	"main": "./src/index.ts",
	"routes": [
		{
			"pattern": "api.gebna.net",
			"custom_domain": true,
		},
	],
	"observability": {
		"logs": {
			"enabled": true,
			"head_sampling_rate": 1,
			"invocation_logs": true,
			"persist": true,
		},
	},
}
